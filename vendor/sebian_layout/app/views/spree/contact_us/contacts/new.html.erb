<script type='text/javascript' src='//maps.google.com/maps/api/js?sensor=false'></script>
<%= render :partial=>"spree/shared/sub_title", :locals=>{:title => Spree.t(:contact_us_title), :breadcrumb_text => Spree.t(:contact_us_title)} %>
<div id="map" class="animate fadeInUp" data-wow-delay="0.4s">
  <input type="hidden" value="<%= Spree::Config[:site_address] %>" id="map-address">
</div>
<div class="tittle">
  <h5>CONTACT US</h5>
  <p>Please don’t hesitate to contact me if you have any questions, commnets or messages. <br>
    I’ll try to respond to everything!</p>
</div>
<section class="contact section-p-30px no-padding-b">
  <div class="container">
    <div class="row">
      <div class="col-sm-12 animate fadeInUp" data-wow-delay="0.4s">
        <%= render :partial => 'spree/shared/error_messages', :locals => { :target => @contact } %>
        <%= render :partial => "spree/shared/contact_form" %>
      </div>
    </div>
  </div>
</section>
